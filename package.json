{
  "name": "registry",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.2.20",
  "scripts": {
    "build": "bun run registry; bun run generate; next build",
    "dev": "next dev --turbopack",
    "format": "prettier --write . --list-different --cache",
    "generate": "bun run scripts/generate-docs.ts",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "postgenerate": "fumadocs-mdx",
    "postinstall": "fumadocs-mdx",
    "registry": "shadcn build",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fumadocs-core": "15.2.12",
    "fumadocs-docgen": "^2.1.0",
    "fumadocs-mdx": "11.6.1",
    "fumadocs-ui": "15.2.12",
    "lucide-react": "^0.503.0",
    "motion": "^12.23.12",
    "next": "15.3.1",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "shadcn": "^2.10.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.18.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prettier-plugin-pkg": "^0.19.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}
